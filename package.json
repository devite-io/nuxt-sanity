{
  "name": "@devite/nuxt-sanity",
  "version": "2.7.0",
  "description": "Advanced Sanity integration for Nuxt.js.",
  "repository": "devite-io/nuxt-sanity",
  "license": "MIT",
  "author": {
    "name": "Justus Geramb <justus@devite.io>",
    "url": "https://www.devite.io"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:cms": "cd playground/cms && pnpm dev",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "pnpm lint && pnpm test && pnpm prepack && changelogen --release && pnpm publish --access=public && git push --follow-tags",
    "lint": "eslint --fix .",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/image": "^1.8.1",
    "@portabletext/vue": "^1.0.11",
    "@sanity/client": "^6.24.1",
    "@sanity/core-loader": "^1.7.22",
    "@sanity/preview-url-secret": "^2.0.5",
    "@sanity/types": "^3.68.3",
    "@sanity/visual-editing": "^2.11.3",
    "defu": "^6.1.4",
    "ofetch": "^1.4.1",
    "ohash": "^1.1.4",
    "unstorage": "^1.14.4"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.7.4",
    "@nuxt/kit": "^3.15.1",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.15.1",
    "@nuxt/test-utils": "^3.15.1",
    "@types/node": "latest",
    "changelogen": "^0.5.7",
    "eslint": "^9.17.0",
    "h3": "^1.13.0",
    "nuxt": "^3.15.1",
    "typescript": "~5.6.3",
    "vitest": "^2.1.8",
    "vitest-environment-nuxt": "1.0.1",
    "vue": "3.5.13",
    "vue-router": "^4.5.0",
    "vue-tsc": "^2.1.6"
  },
  "overrides": {
    "react": "18.3.1"
  },
  "resolutions": {
    "@devite/nuxt-sanity": "link:."
  },
  "build": {
    "externals": [
      "@sanity/client"
    ]
  }
}
