{
  "name": "@devite/nuxt-sanity",
  "version": "2.10.1",
  "description": "Advanced Sanity integration for Nuxt.js.",
  "repository": "devite-io/nuxt-sanity",
  "license": "MIT",
  "author": {
    "name": "Justus Geramb <justus@devite.io>",
    "url": "https://www.devite.io"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/module.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/module.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:cms": "cd playground/cms && pnpm dev",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "pnpm lint && pnpm test && pnpm prepack && git push && changelogen --release && pnpm publish --access=public && git push --follow-tags",
    "lint": "eslint --fix .",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/image": "^1.10.0",
    "@portabletext/vue": "^1.0.12",
    "@sanity/client": "^6.28.4",
    "@sanity/core-loader": "^1.8.1",
    "@sanity/preview-url-secret": "^2.1.7",
    "@sanity/types": "^3.83.0",
    "@sanity/visual-editing": "^2.13.15",
    "defu": "^6.1.4",
    "ofetch": "^1.4.1",
    "ohash": "^1.1.6",
    "unstorage": "^1.15.0"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.7.6",
    "@nuxt/kit": "^3.16.2",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.16.2",
    "@nuxt/test-utils": "^3.17.2",
    "@types/node": "latest",
    "changelogen": "^0.5.7",
    "eslint": "^9.24.0",
    "h3": "^1.15.1",
    "nuxt": "^3.16.2",
    "typescript": "5.6.3",
    "vite": "6.0.1",
    "vitest": "^2.1.9",
    "vitest-environment-nuxt": "1.0.1",
    "vue": "3.5.13",
    "vue-router": "^4.5.0",
    "vue-tsc": "^2.2.8"
  },
  "pnpm": {
    "overrides": {
      "react": "19.0.0",
      "react-dom": "19.0.0"
    }
  },
  "resolutions": {
    "@devite/nuxt-sanity": "link:."
  },
  "build": {
    "externals": [
      "@sanity/client"
    ]
  }
}
