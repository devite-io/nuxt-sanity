{
  "name": "@devite/nuxt-sanity",
  "version": "2.14.2",
  "description": "Advanced Sanity integration for Nuxt.js.",
  "repository": "devite-io/nuxt-sanity",
  "license": "MIT",
  "author": {
    "name": "Justus Geramb <justus@devite.io>",
    "url": "https://www.devite.io"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/module.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/module.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:cms": "cd playground/cms && pnpm dev",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "pnpm lint && pnpm test && pnpm prepack && git push && changelogen --release && pnpm publish --access=public && git push --follow-tags",
    "lint": "eslint --fix .",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/image": "^1.11.0",
    "@portabletext/vue": "^1.0.14",
    "@sanity/client": "^7.12.0",
    "@sanity/core-loader": "^1.8.21",
    "@sanity/preview-url-secret": "^2.1.15",
    "@sanity/types": "^4.11.0",
    "@sanity/visual-editing": "^3.2.4",
    "defu": "^6.1.4",
    "ofetch": "^1.4.1",
    "ohash": "^1.1.6",
    "unstorage": "^1.17.1"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^1.9.0",
    "@nuxt/kit": "^4.1.3",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.1.3",
    "@nuxt/test-utils": "^3.19.2",
    "@types/node": "latest",
    "changelogen": "^0.6.2",
    "eslint": "^9.38.0",
    "h3": "^1.15.4",
    "nuxt": "^4.1.3",
    "typescript": "5.8.3",
    "vite": "7.1.11",
    "vitest": "^4.0.0",
    "vitest-environment-nuxt": "1.0.1",
    "vue": "3.5.22",
    "vue-router": "^4.6.3",
    "vue-tsc": "^3.1.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "esbuild",
      "scrollmirror",
      "sharp",
      "unrs-resolver",
      "yarn"
    ]
  },
  "resolutions": {
    "@devite/nuxt-sanity": "link:."
  },
  "build": {
    "externals": [
      "@sanity/client"
    ]
  }
}
